                        "Ref": "AWS::Region"
                      },
                      ":",
                      {
                        "Ref": "AWS::AccountId"
                      },
                      ":apis/",
                      {
                        "Ref": "apieasyhireGraphQLAPIIdOutput"
                      },
                      "/types/Subscription/*"
