    "LambdaFunction": {
      "Type": "AWS::Lambda::Function",
      "Metadata": {
        "aws:asset:path": "./src",
        "aws:asset:property": "Code"
      },
      "Properties": {
        "Code": {
          "S3Bucket": {
            "Ref": "deploymentBucketName"
          },
          "S3Key": {
            "Ref": "s3Key"
          }
        },
        "Handler": "index.handler",
        "FunctionName": {
          "Fn::If": [
            "ShouldNotCreateEnvResources",
            "helperProfileProgressTrack",
            {
              "Fn::Join": [
                "",
                [
                  "helperProfileProgressTrack",
                  "-",
                  {
                    "Ref": "env"
                  }
                ]
              ]
            }
          ]
